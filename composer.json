{
    "name": "esit/messagetoolbox",
    "description": "Mit dieser Toolbox können Rückmeldungen an den Nutzer einer Seite im Frontend ausgegeben werden. Die Meldungen werden in der Session zwischengespeichert und können z.B. nach dem Redirect beim Speichern eines Formulars ausgegeben werden.",
    "license": "LGPL-3.0-or-later",
    "type": "contao-bundle",
    "authors": [
        {
            "name": "Patrick Froch",
            "email": "info@easySolutionsIT.de",
            "homepage": "http://easySolutionsIT.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "info@easySolutionsIT.de",
        "issues": "https://github.com/eS-IT/databaselayer/issues",
        "docs": "https://github.com/eS-IT/databaselayer/blob/main/README.md"
    },
    "readme": "README.md",
    "require": {
        "php": "~8.0",
        "contao/core-bundle": "~4.13||^5.1"
    },
    "require-dev": {
        "contao/test-case": "^4.13",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Esit\\Messagetoolbox\\": ""
        }
    },
    "extra": {
        "contao-manager-plugin": "Esit\\Messagetoolbox\\Classes\\Contao\\Manager\\Plugin"
    }
}
